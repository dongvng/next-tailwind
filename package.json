{
  "name": "next-tailwind",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "leaflet": "^1.7.1",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "^3.2.2"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.7",
    "@types/node": "17.0.0",
    "@types/react": "17.0.37",
    "autoprefixer": "^10.4.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.6",
    "typescript": "4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
